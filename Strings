name = raw_input("What is your name? ")
location = raw_input("Where do you live? ")
origin = raw_input("Where are you from? ")
education = raw_input("Where do you go to school? ")
major = raw_input("What do you study/what do you wat to study?")
color = raw_input("What is your favorite color? ")

print "Your name is %s, your location is %s, your origin is %s, you study at %s, your studies of interest is %s" \
"and your favorite color is %s, " %(name, location, origin, education, major, color)
